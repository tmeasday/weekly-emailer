<table>
  <thead>
    <tr>
      <th></th>
      <% users.each do |user| %>
        <th><%= user[:name] %></th>
      <% end %>
    </tr>
  </thead>
  
  <tbody>
    <% projects.each do |project| %>
      <tr>
        <th><%= project[:client_name] + ' ' + project[:name] %></th>
        <% users.each do |user| %>
          <td><%= project_user_hours[project[:id]][user[:id]] %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>